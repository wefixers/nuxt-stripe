{
  "name": "@fixers/nuxt-stripe",
  "type": "module",
  "version": "1.1.0",
  "description": "A Nuxt module for Stripe",
  "author": "Luca <git@lucacicada.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wefixers/nuxt-stripe.git"
  },
  "keywords": [
    "nuxt",
    "stripe",
    "stripe-webhook"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    },
    "./server": {
      "types": "./dist/runtime/server/stripe.d.ts",
      "import": "./dist/runtime/server/stripe.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "typecheck": "nuxi typecheck",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.2",
    "@stripe/stripe-js": "^1.54.1",
    "defu": "^6.1.2",
    "h3": "^1.7.1",
    "pathe": "^1.1.1",
    "stripe": "^12.11.0",
    "ufo": "^1.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.4",
    "@nuxt/devtools": "latest",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.5.2",
    "@nuxt/test-utils": "^3.5.1",
    "@t3-oss/env-nuxt": "0.6.0",
    "@types/node": "^18",
    "changelogen": "^0.5.3",
    "eslint": "^8.42.0",
    "nuxt": "^3.5.1",
    "vitest": "^0.31.4",
    "vue-tsc": "^1.8.3",
    "zod": "^3.20.2"
  }
}