{
  "name": "@fixers/nuxt-stripe",
  "type": "module",
  "version": "3.0.3",
  "description": "A Nuxt module for Stripe",
  "author": "Luca <git@lucacicada.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wefixers/nuxt-stripe.git"
  },
  "keywords": [
    "nuxt",
    "stripe",
    "stripe-webhook"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./server": {
      "types": "./dist/runtime/server/stripe.d.ts",
      "import": "./dist/runtime/server/stripe.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "server.d.ts",
    "server.mjs"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "typecheck": "nuxi typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^1.0.8",
    "@nuxt/kit": "^3.9.0",
    "@stripe/stripe-js": "^3.0.7",
    "defu": "^6.1.4",
    "pathe": "^1.1.2",
    "stripe": "^14.19.0",
    "ufo": "^1.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.0",
    "@nuxt/devtools": "latest",
    "@nuxt/devtools-ui-kit": "^1.0.6",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.10.3",
    "@nuxt/test-utils": "^3.11.0",
    "@t3-oss/env-nuxt": "0.7.1",
    "@types/node": "^20.11.22",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "nuxt": "^3.10.3",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27",
    "zod": "^3.22.4"
  }
}
