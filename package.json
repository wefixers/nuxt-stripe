{
  "name": "@fixers/nuxt-stripe",
  "type": "module",
  "version": "4.0.0",
  "packageManager": "pnpm@9.9.0",
  "description": "A Nuxt module for Stripe",
  "author": "Luca <git@lucacicada.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wefixers/nuxt-stripe.git"
  },
  "keywords": [
    "nuxt",
    "stripe",
    "stripe-webhook"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-build-module build --stub && nuxt-module-build prepare && nuxt-module-build build && nuxi generate client",
    "prepack": "pnpm run build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "typecheck": "nuxi typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^1.4.1",
    "@nuxt/kit": "^3.13.1",
    "@stripe/stripe-js": "^4.4.0",
    "defu": "^6.1.4",
    "sirv": "^2.0.4",
    "stripe": "^16.10.0",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.4.1",
    "@nuxt/devtools": "^1.4.1",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.8.3",
    "@nuxt/schema": "^3.13.1",
    "@nuxt/test-utils": "^3.14.1",
    "@tailwindcss/forms": "^0.5.9",
    "@types/node": "^20.16.5",
    "@vueuse/core": "^11.0.3",
    "changelogen": "^0.5.5",
    "eslint": "^9.10.0",
    "nuxt": "^3.13.1",
    "nuxt-shiki": "^0.3.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.1.6",
    "zod": "^3.23.8"
  }
}
