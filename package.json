{
  "name": "@fixers/nuxt-stripe",
  "type": "module",
  "version": "3.0.4",
  "packageManager": "pnpm@9.3.0",
  "description": "A Nuxt module for Stripe",
  "author": "Luca <git@lucacicada.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wefixers/nuxt-stripe.git"
  },
  "keywords": [
    "nuxt",
    "stripe",
    "stripe-webhook"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-build-module build --stub && nuxt-module-build prepare && nuxt-module-build build && nuxi generate client",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "typecheck": "nuxi typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^1.3.9",
    "@nuxt/kit": "^3.12.4",
    "@stripe/stripe-js": "^4.1.0",
    "defu": "^6.1.4",
    "sirv": "^2.0.4",
    "stripe": "^16.5.0",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.24.0",
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/devtools-ui-kit": "^1.0.8",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.4",
    "@nuxt/test-utils": "^3.14.0",
    "@types/node": "^20.14.13",
    "changelogen": "^0.5.5",
    "eslint": "^9.8.0",
    "nuxt": "^3.12.4",
    "nuxt-shiki": "^0.3.0",
    "vitest": "^2.0.4",
    "vue-tsc": "^2.0.29",
    "zod": "^3.23.8"
  },
  "resolutions": {
    "nuxt-og-image": "3.0.0-rc.56"
  }
}
